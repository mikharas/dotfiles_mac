# TMUX ALIASES
alias ts='tmux'         # Start a new unnamed tmux session
alias tsn='tmux new -s' # Start a new named tmux session
alias tl='tmux ls'     # List all active sessions
alias ta='tmux a -t'   # Attach a session (by index or name)
alias tk='tmux kill-session -t' # Kill a session (by index or name)
alias tks='tmux kill-server' # kill all sessions

